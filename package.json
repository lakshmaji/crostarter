{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@hassanmojab/react-modern-calendar-datepicker": "^3.1.7",
    "@inertiajs/core": "^1.0.0",
    "@inertiajs/inertia": "0.11.1",
    "@inertiajs/inertia-react": "0.8.1",
    "@inertiajs/progress": "^0.2.7",
    "@inertiajs/server": "0.1.0",
    "@react-icons/all-files": "^4.1.0",
    "chroma-js": "^2.4.2",
    "cookie-though": "^1.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.38.0",
    "react-icons": "^4.7.1",
    "react-masonry-css": "^1.0.16",
    "react-select": "^5.6.1",
    "recharts": "2.4.1"
  },
  "scripts": {
    "clean": "npx rimraf node_modules public/vite-* public/vite",
    "clean:build": "npx rimraf public/vite-* public/vite tmp/cache/vite",
    "build:ssr": "bin/vite build --ssr",
    "build:csr": "bin/vite build",
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "dev": "bin/vite dev",
    "dev:ssr": "bin/vite ssr",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint app/javascript",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier app/javascript --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick --staged",
    "analyze": "source-map-explorer 'public/vite/assets/*.js'",
    "typecheck": "tsc -b",
    "ci:build:ssr": "vite build --ssr",
    "ci:build:csr": "vite build"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.1",
    "@types/chroma-js": "^2.1.4",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-virtualized": "^9.21.21",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.26.0",
    "@vitejs/plugin-react": "2.1.0",
    "esbuild": "0.15.10",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "^8.0.2",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "sass": "1.56.1",
    "source-map-explorer": "^2.5.3",
    "typescript": "4.9.4",
    "vite": "3.0.0",
    "vite-plugin-ruby": "3.1.0",
    "vite-tsconfig-paths": "3.6.0"
  }
}